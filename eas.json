{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "image": "latest"
    },
    "development:simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "image": "latest"
    },
    "preview": {
      "distribution": "internal",
      "image": "latest"
    },
    "production": {
      "autoIncrement": true,
      "image": "latest"
    }
  },
  "submit": {
    "production": {}
  }
}
