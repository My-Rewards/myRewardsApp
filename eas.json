{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "beta"
      }
    },
    "development:simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "prod"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6744338057",
        "bundleIdentifier": "com.myrewards.app"
      },
      "android": {
        "track": "production",
        "releaseStatus": "completed"
      }
    }
  }
}
